{
  "expo": {
    "name": "BPay",
    "slug": "bpay-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/5782897843587714011_120.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/5782897843587714011_120.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#1a365d"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take profile photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select profile photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/5782897843587714011_120.jpg",
        "backgroundColor": "#1a365d"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/images/5782897843587714011_120.jpg"
    },
    "platforms": ["ios", "android", "web"]
  }
}